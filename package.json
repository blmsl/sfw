{
  "author": "Thomas Handle <thomas.handle@mail.com>",
  "name": "sfw-backend",
  "description": "Admin backend for sfwinterbach.com",
  "homepage": "http://www.sfwinterbach.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Meistercoach83/sfw.git"
  },
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "analyzer": "webpack-bundle-analyzer dist/stats.json",
    "ng": "ng",
    "rimraf": "rimraf",
    "changelog": "standard-changelog",
    "start": "ng serve",
    "start:hmr": "ng serve --hmr",
    "start:aot": "ng serve --aot",
    "start:prod": "ng serve --prod",
    "start:prod:aot": "ng serve --prod --aot",
    "build": "ng build",
    "build:client-and-server-bundles": "ng build --prod && ng run sfw:server:production",
    "build:prerender": "npm run build:client-and-server-bundles && npm run compile:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "compile:server": "tsc -p server.tsconfig.json",
    "generate:prerender": "cd dist && node prerender",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server",
    "build:prod": "npm run build --prod",
    "build:prod:aot": "npm run build -- --aot",
    "build:ci": "npm run build:prod && npm run build:prod:aot",
    "build:demo": "npm run build:prod:aot --",
    "test": "ng test",
    "test:coverage": "npm run clean:coverage && ng test -sr -cc",
    "lint": "ng lint",
    "lint:styles": "stylelint ./src/app/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "clean:dist": "npm run rimraf -- dist",
    "clean:coverage": "npm run rimraf -- coverage",
    "postinstall": "npm run tsformat",
    "tsformat": "tsfmt -r",
    "pwa": "ng build --prod && sw-precache --root=dist --config=precache-config.js"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.5",
    "@angular-devkit/build-angular": "^0.10.3",
    "@angular/animations": "^7.0.1",
    "@angular/cdk": "^7.0.2",
    "@angular/cli": "^7.0.4",
    "@angular/common": "^7.0.1",
    "@angular/compiler": "^7.0.1",
    "@angular/core": "^7.0.1",
    "@angular/fire": "^5.1.0",
    "@angular/flex-layout": "^7.0.0-beta.19",
    "@angular/forms": "^7.0.1",
    "@angular/http": "^7.0.1",
    "@angular/material": "^7.0.2",
    "@angular/material-moment-adapter": "^7.0.2",
    "@angular/platform-browser": "^7.0.1",
    "@angular/platform-browser-dynamic": "^7.0.1",
    "@angular/platform-server": "^7.0.1",
    "@angular/pwa": "^0.10.3",
    "@angular/router": "^7.0.1",
    "@angular/service-worker": "^7.0.1",
    "@ng-toolkit/pwa": "^1.1.42",
    "@ng-toolkit/universal": "^1.1.42",
    "@nguniversal/common": "^7.0.2",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
    "@ngx-loading-bar/router": "^2.2.0",
    "@ngx-translate/core": "^11.0.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "@swimlane/ngx-datatable": "^14.0.0",
    "@types/node": "^10.12.1",
    "angular-froala-wysiwyg": "^2.9.0",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.7",
    "firebase": "^5.5.7",
    "font-awesome": "^4.7.0",
    "fullcalendar": "3.6.1",
    "hammerjs": "^2.0.8",
    "highlightjs": "^9.10.0",
    "jquery": "^3.3.1",
    "moment": "^2.22.2",
    "ng-animate": "^0.3.4",
    "ng-fullcalendar": "^1.7.0",
    "ng-pick-datetime": "^7.0.0",
    "ng-pick-datetime-moment": "^1.0.8",
    "ng2-charts": "^1.6.0",
    "ng2-validation": "^4.2.0",
    "ngx-bootstrap": "^3.1.1",
    "ngx-chips": "1.9.8",
    "ngx-infinite-scroll": "^6.0.1",
    "ngx-perfect-scrollbar": "^7.0.0",
    "ngx-pipes": "^2.3.6",
    "ngx-timeago": "^1.0.0",
    "rxjs": "^6.3.3",
    "screenfull": "^3.3.3",
    "ts-loader": "^5.3.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^7.0.2",
    "@types/jquery": "^3.3.22",
    "codelyzer": "^4.4.4",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.1.4",
    "typescript-formatter": "^7.2.2"
  }
}
